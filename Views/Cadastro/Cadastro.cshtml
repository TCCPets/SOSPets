@{
    ViewData["Title"] = "Cadastro";
}
<link rel="stylesheet" href="/css/cadastro.css">
<section class="container">
        <div class="login-container">
            <div class="circle circle-one"></div>
            <div class="form-container">
                <img src="/img/cachorro-removebg-preview.png" alt="illustration" class="illustration" style="margin-top: 50%;" />
                <P class="opacity">CADASTRO</P>
        
                <form>
                    <input type="text" placeholder="Nome" />
                    <input type="text" placeholder="Sobrenome" />
                    <input type="email" placeholder="E-mail" />
                    <input type="text" placeholder="Telefone" />
                    <input type="password" placeholder="Senha" />
                    <input type="password" placeholder="Repetir Senha" />
                    <button class="opacity">Cadastrar</button>
                </form>

                <div class="register-forget opacity">
                    <a class="nav-link text-green" asp-area="" asp-controller="Login" asp-action="Login">Faça seu Login</a>
                </div>

            </div>
            <div class="circle circle-two"></div>
        </div>
        <div class="theme-btn-container"></div>
    </section>
    <script type="module" src="/js/Login.js"></script>